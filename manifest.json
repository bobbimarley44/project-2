{
  "manifest_version": 2,
  "name": "Dead Namerator",
  "description": "Replaces dead name & pronouns",
  "version": "0.1",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file://*"
      ],
      "js": [
        "main-content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "options_page": "options.html",
  "icons": {
    "128": "images/heart.png"
  }
}
